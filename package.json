{
  "name": "assignment",
  "version": "1.0.0",
  "private": true,
  "description": "Assignment",
  "scripts": {
    "dev": "npm run start:dev",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"public/**/*.css\" --syntax css",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --watchAll=false"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax css",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.10",
    "@umijs/route-utils": "^2.0.0",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "umi": "^3.5.0",
    "umi-request": "^1.4.0",
    "use-state-if-mounted": "^1.0.7"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.8.0",
    "@umijs/openapi": "^1.3.0",
    "@umijs/plugin-blocks": "^2.2.0",
    "@umijs/plugin-esbuild": "^1.4.0",
    "@umijs/plugin-openapi": "^1.3.0",
    "@umijs/preset-dumi": "^1.1.0",
    "@umijs/preset-react": "^1.8.17",
    "@umijs/yorkie": "^2.0.5",
    "cross-env": "^7.0.0",
    "eslint-config-xo": "^0.38.0",
    "eslint-plugin-react": "^7.31.10",
    "lint-staged": "^10.0.0",
    "stylelint": "^13.0.0",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
